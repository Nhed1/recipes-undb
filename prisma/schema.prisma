// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:123@localhost:32769/recipes"
}

model Recipe {
  id            Int      @id @default(autoincrement())
  name          String
  diabetesLevel Int
  ingredients   String
  description   String
}